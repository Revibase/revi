{
  "expo": {
    "name": "magpie",
    "slug": "magpie",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "config": {
        "usesNonExemptEncryption": false
      },
      "supportsTablet": true,
      "associatedDomains": [
        "webcredentials:horizon.blinksfeed.com"
      ],
      "bundleIdentifier": "com.jychab.magpie",
      "newArchEnabled": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "android.permission.NFC"
      ],
      "package": "com.jychab.magpie",
      "newArchEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "react-native-nfc-manager",
        {
          "nfcPermission": "NFC is required to sign transactions.",
          "selectIdentifiers": [
            "A0000003965453000000010300000000",
            "D276000085304A434F9003"
          ],
          "includeNdefEntitlement": true
        }
      ],
      "expo-router",
      "expo-font",
      [
        "expo-build-properties",
        {
          "newArchEnabled": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "091ab3d8-0e6f-4ad7-a9d8-f8c14cc70070"
      }
    },
    "owner": "jychab"
  }
}
